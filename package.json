{
  "name": "aoc-2017",
  "version": "1.0.0",
  "description": "Advent of code 2017",
  "main": "index.js",
  "scripts": {
    "test": "mocha --opts mocha.opts",
    "coverage": "nyc mocha --opts mocha.opts",
    "watch": "npm test -- -w --watch-extensions ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alocquet/aoc-2017.git"
  },
  "author": "alocquet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alocquet/aoc-2017/issues"
  },
  "homepage": "https://github.com/alocquet/aoc-2017#readme",
  "config": {
    "mocha-sonar-reporter": {
      "outputfile": "reports/Test-all.xml"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "4.0.6",
    "@types/graceful-fs": "2.0.29",
    "@types/mocha": "2.2.44",
    "@types/sinon": "4.0.0",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-sonar-reporter": "^0.1.6",
    "nyc": "^11.3.0",
    "sinon": "4.1.2",
    "source-map-support": "^0.5.0",
    "ts-node": "3.3.0",
    "typescript": "2.6.2"
  },
  "dependencies": {
    "graceful-fs": "4.1.11",
    "napajs": "^0.1.7",
    "rxjs": "^5.5.2",
    "tslint": "^5.8.0"
  }
}
